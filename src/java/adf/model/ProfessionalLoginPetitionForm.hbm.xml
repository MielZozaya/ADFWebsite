<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="adf.model">
  <class name="ProfessionalLoginPetitionForm">
    <id name="email"/>
    <property column="version" name="version"/>
    <property name="name"/>
    <property name="middleName"/>
    <property name="surname"/>
    <property name="telephoneNumber"/>
    <property name="organization"/>
    <property name="positionHold"/>
    <property name="personalStatement"/>
    <property name="creationDate"/>
    <property name="acceptedPetition"/>
  </class>
</hibernate-mapping>
