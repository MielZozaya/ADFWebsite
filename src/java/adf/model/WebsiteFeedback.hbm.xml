<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="adf.model">
  <class name="WebsiteFeedback" table="website_feedback">
    <id column="website_feedback_id" name="id">
      <generator class="sequence"/>
    </id>
    <version column="version" name="version"/>
    <property column="username" name="user"/>
    <property column="text" name="text" not-null="true"/>
    <property column="stars" name="stars" not-null="true"/>
  </class>
</hibernate-mapping>
